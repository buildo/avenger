    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "dtslint": "github:gcanti/dtslint",
    "fast-check": "^1.17.0",
    "fp-ts-laws": "0.1.0",
    "jest": "^25.1.0",
    "prettier": "^1.18.0",
