{
  "name": "avenger",
  "version": "2.2.0",
  "description": "A CQRS-flavoured data fetching and caching layer in JavaScript. Batching, caching, data-dependecies and manual invalidations in a declarative fashion for node and the browser",
  "scripts": {
    "lint": "eslint src test",
    "test": "jest",
    "build": "rm -rf lib && mkdir lib && babel --out-dir lib src && (cd src; tar cf - *.d.ts **/*.d.ts) | (cd lib; tar xf -)",
    "preversion": "npm run lint && npm test",
    "prepublish": "npm run build"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "lodash": "^4.13.1",
    "rxjs": "^5.4.2",
    "typelevel-ts": "^0.2.3"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.2",
    "babel-cli": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-jest": "^22.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "2.9.0",
    "eslint-config-buildo": "github:buildo/eslint-config",
    "io-ts": "^1.0.5",
    "jest": "^22.0.4",
    "sinon": "1.17.4",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.1"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./test/jestSetup.js",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
